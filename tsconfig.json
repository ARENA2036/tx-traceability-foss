{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "@core/*": [
        "src/app/modules/core/*"
      ],
      "@layout/*": [
        "src/app/modules/core/layout/*"
      ],
      "@page/*": [
        "src/app/modules/page/*"
      ],
      "@shared/*": [
        "src/app/modules/shared/*"
      ],
      "@shared": [
        "src/app/modules/shared/index"
      ],
      "@env": [
        "src/environments/environment"
      ]
    }
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInjectionParameters": true
  }
}
