{
  "name": "webapp",
  "version": "2.1.1",
  "scripts": {
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/webapp/stats.json",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --configuration production",
    "format": "prettier --config .prettierrc --write \"src/**/*.{js,ts,json,md,css,html}\"",
    "generate-icons": "svg-to-ts",
    "lint": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "ng": "ng",
    "ng-cdk": "ng update --force @angular/cdk",
    "ng-cli": "ng update --force @angular/cli",
    "ng-core": "ng update --force @angular/core",
    "ng-material": "ng update --force @angular/material",
    "start": "ng serve",
    "sonar": "sonar-scanner",
    "test": "ng test --code-coverage --watch=true --browsers=Chrome",
    "test:ci": "ng test --code-coverage --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "7.0.1",
    "@angular/animations": "13.3.6",
    "@angular/cdk": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/material": "13.3.6",
    "@angular/material-moment-adapter": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "@auth0/angular-jwt": "5.0.2",
    "@deck.gl/core": "8.7.8",
    "@deck.gl/layers": "8.7.8",
    "@deck.gl/mapbox": "8.7.8",
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "@types/file-saver": "2.0.5",
    "@types/lodash-es": "4.17.6",
    "autoprefixer": "10.4.7",
    "country-code-lookup": "0.0.20",
    "d3": "7.4.4",
    "file-saver": "2.0.5",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "keycloak-angular": "9.1.0",
    "keycloak-js": "18.0.0",
    "lodash-es": "4.17.21",
    "luxon": "2.3.2",
    "mapbox-gl": "2.8.2",
    "moment": "2.29.3",
    "ngx-clipboard": "15.1.0",
    "ngx-daterangepicker-material": "2.4.2",
    "ngx-mapbox-gl": "7.1.2",
    "postcss": "8.4.13",
    "puppeteer": "13.7.0",
    "rxjs": "7.5.5",
    "tailwindcss": "3.0.24",
    "tslib": "2.4.0",
    "uuid": "8.3.2",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.6",
    "@angular/language-service": "13.3.6",
    "@ngneat/svg-icon": "4.1.2",
    "@types/d3": "7.1.0",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/luxon": "2.3.2",
    "@types/mapbox-gl": "2.7.1",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "7.0.4",
    "jasmine-core": "4.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "msw": "^0.39.2",
    "postcss-import": "14.1.0",
    "postcss-loader": "6.2.1",
    "postcss-scss": "4.0.4",
    "sonar-scanner": "3.1.0",
    "stylelint": "14.8.2",
    "stylelint-config-recommended": "7.0.0",
    "svg-to-ts": "8.6.1",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "svg-to-ts": {
    "generateType": "false",
    "delimiter": "KEBAB",
    "conversionType": "files",
    "iconsFolderName": "svg",
    "prefix": "app",
    "srcFiles": [
      "./src/assets/svg/*.svg"
    ],
    "outputDirectory": "./src/app",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  },
  "msw": {
    "workerDirectory": "src"
  }
}